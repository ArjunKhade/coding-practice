package dsa;

public class SinglyLinkedList {
	
	      public Node head;
	      public int size;
	      
	      public SinglyLinkedList(Node head) {
				this.head = null;
			}
	
	   public void addAtStart(int data) {
		   Node newNode = new Node(data);
		   newNode.next=head;
		   head=newNode;
		   size++;
	   }
	
	
	
	
	
	
	
	
	
	
	

	

	public static void main(String[] args) {
		
	
		

	}
	
	class Node{
		 
		int data;
		Node next;
		
		public Node(int data) {
			super();
			this.data = data;
			this.next = null;
		}
		
		
	}
	
	

}
